{
  "version": 1,
  "paths": {
    "/user": {
      "name": "/user",
      "lambdaFunction": "createUser",
      "permissions": {
        "setting": "open"
      }
    },
    "/user/{id}": {
      "name": "/user/{id}",
      "lambdaFunction": "getUser",
      "permissions": {
        "setting": "open"
      }
    }
  }
}